<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
	<Style x:Key="MenuIcon" TargetType="Image">
		<Setter Property="Height" Value="16" />
		<Setter Property="Width" Value="16" />
	</Style>
	<Image x:Key="GenericDocument"
		   Source="../Images/Icons/GenericDocument.png"
		   Style="{StaticResource MenuIcon}" />
	<Image x:Key="Save"
		   Source="../Images/Icons/Save.png"
		   Style="{StaticResource MenuIcon}" />
	<Image x:Key="OpenProject"
		   Source="../Images/Icons/OpenFolder.png"
		   Style="{StaticResource MenuIcon}" />
	
	<Image x:Key="Cut"
		   Source="../Images/Icons/Cut.png"
		   Style="{StaticResource MenuIcon}" />
	<Image x:Key="Copy"
		   Source="../Images/Icons/Copy.png"
		   Style="{StaticResource MenuIcon}" />
	<Image x:Key="Paste"
		   Source="../Images/Icons/Paste.png"
		   Style="{StaticResource MenuIcon}" />
	<Image x:Key="Delete"
		   Source="../Images/Icons/Delete.png"
		   Style="{StaticResource MenuIcon}" />
</ResourceDictionary>